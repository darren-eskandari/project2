<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <%- include('../partials/navbar.ejs') %>
    <header>
      <h1>Photos</h1>

    </header>

    <main>

      <section>
        <% for(let i = 0; i < photos.length; i++){ %>
          <div class="img-index">
            <a href="/photos/<%=photos[i]._id%>"><img src="<%=photos[i].img%>" /></a>
          </div>
        <% } %>
      </section>
      <% if(isLogged) { %>
        <a href="/photos/new">new photos</a>
      <% } %>
      </main>
    
    <aside>
        <% if(isLogged) { %>
          my photos
        <% } %>
    </aside>
  </body>
</html>