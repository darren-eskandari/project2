<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <%- include('../partials/navbar.ejs') %>
    <main>
      <% if(isLogged) { %>
        <a class="button" href="/photos/new">Add Photos</a>
      <% } %>

    <section class="gallery">
      <% for(let i = 0; i < photos.length; i++){ %>
        <div class="img-index">
          <a href="/photos/<%=photos[i]._id%>"><img src="<%=photos[i].img%>" /></a>
        </div>
      <% } %>
    </section>
    </main>
  </body>
</html>